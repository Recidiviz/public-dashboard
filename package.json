    "@reach/menu-button": "^0.10.5",
    "@reach/router": "^1.3.4",
    "@w11r/use-breakpoint": "^1.7.0",
    "cache-manager": "^3.3.0",
    "classnames": "^2.2.6",
    "cors": "^2.8.5",
    "d3-array": "^2.4.0",
    "d3-color": "^1.4.1",
    "d3-dsv": "^2.0.0",
    "d3-force": "^2.0.1",
    "d3-force-limit": "^1.1.3",
    "d3-format": "^1.4.4",
    "d3-geo": "^1.12.1",
    "d3-scale": "^3.2.1",
    "date-fns": "^2.14.0",
    "deepmerge": "^4.2.2",
    "dotenv": "^8.2.0",
    "empty-lite": "^1.0.3",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "express-easy-zip": "^1.1.5",
    "helmet": "^3.23.3",
    "morgan": "^1.10.0",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-collapsed": "^3.0.0",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-measure": "^2.3.0",
    "react-scripts": "3.4.3",
    "react-simple-maps": "^2.1.2",
    "react-sticky": "^6.0.3",
    "semiotic": "^1.20.5",
    "set-order": "^0.3.5",
    "styled-components": "^5.1.1",
    "styled-normalize": "^8.0.7",
    "terser-webpack-plugin": "^1.4.3",
    "topojson": "^3.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": ">=4",
    "lint-staged": ">=10",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css,scss,md}": [
      "eslint --fix"
    ],
    "src/**/*.json": [
      "prettier --write"
    ]
  },
  "resolutions": {
    "styled-components": "^5"
  }
}
