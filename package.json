{
  "name": "public-dashboard",
  "description": "A public-facing dashboard to help educate citizens and build accountability",
  "version": "0.1.0",
  "private": true,
  "repository": "git@github.com:Recidiviz/public-dashboard.git",
  "author": "Recidiviz <team@recidiviz.org>",
  "license": "GNU General Public License v3",
  "scripts": {
    "start": "node server.js",
    "build": "react-scripts build",
    "build-staging": "env-cmd -f .env.development react-scripts build",
    "spa": "react-scripts start",
    "server": "node server.js",
    "server:dev": "nodemon server.js",
    "dev": "npm-run-all --parallel spa server:dev",
    "test": "react-scripts test",
    "lint": "eslint .",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@accessible/slider": "^2.0.2",
    "@google-cloud/storage": "^5.1.2",
